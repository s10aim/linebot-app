<div class="post-container mb-3">
  <h1>編集</h1>
  <%= @post.title %></p>
<% @post.images.each_with_index do |image, index| %>
  <div class="edit">
    <%= image_tag image.url, class: "post-img" %>
    <div id="<%= index %>" class="edit-btn">
      <i class="fas fa-times-circle fa-lg"></i>
    </div>
  </div>
<% end %>
</div>
<%= render 'form' %>
